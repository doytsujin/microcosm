{
  "name": "microcosm-devtools",
  "version": "0.0.0",
  "description": "Devtools for Microcosm",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config shells/dev/webpack.config.js",
    "dev:chrome": "webpack --watch --hide-modules --config shells/chrome/webpack.config.js",
    "build": "./bin/bundle",
    "release": "yarn build && yarn zip"
  },
  "keywords": [
    "microcosm",
    "devtools"
  ],
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^7.0.0",
    "circular-json-es6": "^2.0.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.4",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "microcosm": "^12.9.0",
    "paths-js": "^0.4.5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "3.1.3",
    "react-json-tree": "^0.11.0",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.4.5"
  }
}
